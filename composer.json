{
    "name": "dynamic/silverstripe-elemental-stat-counters",
    "description": "Statistic counters block for Silverstripe Elemental",
    "authors": [
        {
            "name": "Dynamic",
            "email": "dev@dynamicagency.com",
            "homepage": "http://www.dynamicagency.com"
        }
    ],
    "keywords": [
        "silverstripe", "CMS", "elemental", "statistic", "counter"
    ],
    "type": "silverstripe-vendormodule",
    "license": "BSD-3-Clause",
    "require": {
        "dnadesign/silverstripe-elemental": "^4.0"
    },
    "require-dev": {
        "silverstripe/recipe-testing": "^2",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "config": {
        "process-timeout": 600
    },
    "autoload": {
        "psr-4": {
            "Dynamic\\Elements\\StatCounters\\": "src/",
            "Dynamic\\Elements\\StatCounters\\Test\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "expose": [
            "client/src"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
